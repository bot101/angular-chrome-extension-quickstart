{
  "name": "Angular Extension",
  "version": "0.0.1",
  "description": "Build an Extension with Angular",
  "manifest_version": 2,
  "permissions": ["activeTab", "webNavigation"],
  "browser_action": {
    "default_popup": "index.html"
  },
  "background": {
    "scripts": ["background.js", "runtime.js"]
  },
  "options_ui": {
    "page": "/index.html?page=options"
  }
}
